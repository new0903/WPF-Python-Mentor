<UserControl x:Class="WpfAppPythone.Templates.RadioSelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfAppPythone.Templates"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:models="clr-namespace:WpfAppPythone.AppData.Models"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <RadioButton FontSize="16" x:Name="RadioSelectorQuest" GroupName="RadioSelectorQuest" Checked="RadioSelectorQuest_Checked"  Content="{Binding}"/>
